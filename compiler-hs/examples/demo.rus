# Demo of RuScript


class example {
    fn add(self, u, v) {
        return u + v;
    }
};

obj = new example;

class example2 {
    fn bridge(self, x, y) {
        global obj;
        return obj.add(x, y);
    }
};


obj2 = new example2;

a = 1;
b = 2;

c = obj2.bridge(a, b); # c should be 3
print c;


