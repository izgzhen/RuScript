# Demo of RuScript

class Y {
    virtual fn minus(y: Int) -> Int
}

class X inherits Y {
    private a : Int
    b : Bool

    fn add(x: Int) -> Int {
        var a: Int = self.a;

        if (self.b) {
            return a.add(x);
        }
        else {
            return x;
        }
    }

    fn minus(y: Int) -> Int {
        return y;
    }
}


var a : Int = 1;
var b : Bool = True;

var c : X = new X(a, b);

a = 2;

while(b) {
    a = c.minus(a);
}

