# Demo of RuScript

class X inherits Y {
    private a : Int
    b : Bool

    fn add(x: Int) {
        if (self.b) {
            return a.add(x);
        }
        else {
            return x;
        }
    }

    virtual fn minus(y: Int)
}


var a : Int = 1;
var b : Bool = True;

var c : X = new X(a, b);

a = 2;

while(b) {
    c.print();
}

